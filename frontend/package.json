{
  "name": "apyhub-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "cd .. && cp .env.example .env || echo '.env already exists'",
    "predev": "node -e \"const fs=require('fs'); const path=require('path'); const rootEnv=path.join(__dirname,'../.env'); const localEnv=path.join(__dirname,'.env.local'); if(fs.existsSync(rootEnv)&&!fs.existsSync(localEnv)){fs.copyFileSync(rootEnv,localEnv);console.log('✓ Copied root .env to .env.local')}\"",
    "prebuild": "node -e \"const fs=require('fs'); const path=require('path'); const rootEnv=path.join(__dirname,'../.env'); const localEnv=path.join(__dirname,'.env.local'); if(fs.existsSync(rootEnv)&&!fs.existsSync(localEnv)){fs.copyFileSync(rootEnv,localEnv);console.log('✓ Copied root .env to .env.local')}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "framer-motion": "^11.3.28",
    "next": "^14.2.33",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.3.0",
    "recharts": "^3.3.0",
    "viem": "^2.38.4",
    "wagmi": "^2.18.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5"
  },
  "overrides": {
